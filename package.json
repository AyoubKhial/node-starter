{
    "name": "node-starter",
    "version": "1.0.0",
    "description": "",
    "main": "src/app.js",
    "scripts": {
        "build": "npm run clean && npm run transpile",
        "clean": "rm -rf dist",
        "dev": "SET \"NODE_ENV=development\" && nodemon -r dotenv/config --exec babel-node ./src/app.js",
        "lint": "eslint --fix . && echo 'Lint complete.'",
        "server": "node ./dist/app.js",
        "start": "npm run build && npm run server",
        "transpile": "babel ./src --out-dir dist"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/AyoubKhial/node-starter.git"
    },
    "author": "Ayoub KHIAL",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/AyoubKhial/node-starter/issues"
    },
    "homepage": "https://github.com/AyoubKhial/node-starter#readme",
    "devDependencies": {
        "@babel/cli": "^7.11.6",
        "@babel/core": "^7.11.6",
        "@babel/node": "^7.10.5",
        "@babel/preset-env": "^7.11.5",
        "dotenv": "^8.2.0",
        "eslint": "^7.9.0",
        "eslint-config-airbnb-base": "^14.2.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-prettier": "^3.1.4",
        "nodemon": "^2.0.4",
        "prettier": "^2.1.2"
    },
    "dependencies": {
        "express": "^4.17.1",
        "mongoose": "^5.10.6",
        "morgan": "^1.10.0",
        "winston": "^3.3.3"
    }
}
